{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "DATABASE_URL": "sqlite+aiosqlite:///:memory:",
    "VERCEL": "true"
  },
  "functions": {
    "main.py": {
      "maxDuration": 30
    }
  }
} 